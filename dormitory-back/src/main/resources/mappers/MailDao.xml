<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.ncc.zsh.dao.MailDao">
    <insert id="insert">
        insert into email(
            sender,
            receiver,
            subject,
            text
        )values(
                   #{sender},
                   #{receiver},
                   #{subject},
                   #{text}
               )
    </insert>
</mapper>